<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="WSOperacionesPortBinding" />
                <binding name="WSRecetaPortBinding" />
                <binding name="WSInventarioPortBinding" />
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:8080/ServicioWeb_SOAP/WSOperaciones"
                binding="basicHttpBinding" bindingConfiguration="WSOperacionesPortBinding"
                contract="ServicioWebSOAPOperaciones.WSOperaciones" name="WSOperacionesPort" />
            <endpoint address="http://localhost:8080/ServicioWeb_SOAP/WSReceta"
                binding="basicHttpBinding" bindingConfiguration="WSRecetaPortBinding"
                contract="ServicioWebSOAPReceta.WSReceta" name="WSRecetaPort" />
            <endpoint address="http://localhost:8080/ServicioWeb_SOAP/WSInventario"
                binding="basicHttpBinding" bindingConfiguration="WSInventarioPortBinding"
                contract="ServicioWebSOAPInventario.WSInventario" name="WSInventarioPort" />
        </client>
    </system.serviceModel>
</configuration>